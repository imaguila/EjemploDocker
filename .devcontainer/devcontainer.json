{
  "name": "Mi Fullstack Stack",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "backend",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "shutdownAction": "none",
  "forwardPorts": [80, 8000],
  "portsAttributes": {
    "80": { "label": "Frontend", "onAutoForward": "openBrowser" },
    "8000": { "label": "Backend", "visibility": "public" }
  }
}